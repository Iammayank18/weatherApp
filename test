  const { temp } = data.main;
      var temp_celsius = temp - 273.15;
      var temp_celsius = temp_celsius.toFixed(2);
      var temp_celsius = temp_celsius + "°C";
      document.getElementById("temprature").innerHTML = temp_celsius;
      var temp_fahrenheit = (temp - 273.15) * 1.8 + 32;
      var temp_fahrenheit = temp_fahrenheit.toFixed(2);
      var temp_fahrenheit = temp_fahrenheit + "°F";

